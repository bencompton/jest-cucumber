{
  "name": "jest-cucumber-examples",
  "version": "1.0.0",
  "description": "Execute Gherkin scenarios in Jest",
  "main": "dist/src/index.js",
  "scripts": {
    "test": "jest --color",
    "jest": "jest --verbose"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-jest": "^26.2.2",
    "babel-plugin-module-resolver": "^4.0.0",
    "babel-preset-env": "^1.7.0"
  },
  "jest": {
    "transform": {
      "^.+\\.jsx?$": "babel-jest"
    },
    "testMatch": [
      "**/*.steps.js"
    ],
    "moduleFileExtensions": [
      "js",
      "jsx"
    ]
  },
  "dependencies": {
    "jest": "^26.2.2"
  }
}
