{
  "name": "jest-cucumber",
  "version": "2.0.11",
  "description": "Execute Gherkin scenarios in Jest",
  "main": "dist/src/index.js",
  "typings": "dist/src/index.d.ts",
  "scripts": {
    "build": "tsc",
    "jest": "jest --verbose",
    "test": "npm run build & npm run lint & jest --color",
    "lint": "tslint --project ./"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/bencompton/jest-cucumber.git"
  },
  "keywords": [
    "jest",
    "gherkin",
    "cucumber"
  ],
  "author": "Ben Compton",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/bencompton/jest-cucumber/issues"
  },
  "homepage": "https://github.com/bencompton/jest-cucumber#readme",
  "devDependencies": {
    "babel-jest": "^24.1.0",
    "ts-jest": "^24.0.0",
    "tslint": "^5.10.0",
    "typescript": "^3.3.3"
  },
  "dependencies": {
    "@types/jest": "^24.0.7",
    "@types/node": "^11.9.4",
    "callsites": "^3.0.0",
    "gherkin": "^5.0.0",
    "jest": "^24.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testMatch": [
      "**/*.steps.ts"
    ],
    "moduleFileExtensions": [
      "js",
      "ts",
      "tsx"
    ]
  }
}
